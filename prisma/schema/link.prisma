model Link {
  id          String           @id @default(cuid())
  createdAt   DateTime         @default(now())
  updatedAt   DateTime         @updatedAt
  //
  name        String?
  note        String?
  ogUrl       String?
  toUrl       String?
  expires     DateTime?
  //
  createdBy   WorkspaceMember? @relation(fields: [createdById], references: [id], onDelete: SetNull)
  createdById String?
  //
  workspace   Workspace        @relation(fields: [workspaceId], references: [id], onDelete: Cascade)
  workspaceId String
}
